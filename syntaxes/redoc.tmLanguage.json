{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "redoc",
	"patterns": [
		{ "include": "#escape" },
		{ "include": "#comment" },
		{ "include": "#decl_block" },
		{ "include": "#region_block_note" },
		{ "include": "#region_block_example" },
		{ "include": "#region_footnote" },
		{ "include": "#inline_code" },
		{ "include": "#placeholder" },
		{ "include": "#exposid" },
		{ "include": "#grammar" },
		{ "include": "#codeblock" },
		{ "include": "#codeblock_literal" },
		{ "include": "#syntax" },
		{ "include": "#table" },
		{ "include": "#math" },
		{ "include": "#macro" },
		{ "include": "#directive" },
		{ "include": "#langtag" },
		{ "include": "#blocktag" },
		{ "include": "#inlinetag" },
		{ "include": "#ref" },
		{ "include": "#var" },
		{ "include": "#def" },
		{ "include": "#cite" },
		{ "include": "#eval" },
		{ "include": "#url" },
		{ "include": "#span" },
		{ "include": "#index" }
	],
	"repository": {
		"escape": {
			"match": "`\\[|`\\]|``|`,",
			"name": "markup.escape.redoc"
		},
		"comment": {
			"begin": "\\[\\/",
			"end": "\\/\\]",
			"name": "comment.block.redoc"
		},
		"eval": {
			"begin": "\\[\\?([_a-zA-Z][_a-zA-Z0-9]*)((?:@(?:\\w+)(?:=(?:[^@#\\s]+))?)*)(#[-a-zA-Z.:\\$]+)?",
			"end": "\\]",
			"name": "markup.inline.tag.redoc",
			"contentName": "markup.eval.content.redoc",
			"captures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "support.class.redoc" },
				"3": { "name": "markup.name.redoc" }
			},
			"patterns": [
				{ "include": "#escape" },
				{ "include": "#macro" },
				{ "include": "#span" },
				{ "include": "#grammar" },
				{ "include": "#subindex" },
				{ "include": "#placeholder" },
				{ "include": "#exposid" },
				{ "include": "#inline_code" }
			]
		},
		"inline_code": {
			"begin": "\\[`((?::opt|:key|:c){,2})((?:@def|@lib){,2})?(?=\\b|\\W)",
			"end": "\\]",
			"name": "markup.inline.tag.redoc",
			"contentName": "markup.code.inline.redoc",
			"captures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "support.class.redoc" }
			},
			"patterns": [
				{ "include": "#escape" },
				{"include": "#macro"},
				{"include": "#inline_code"},
				{"include": "#grammar"},
				{ "include": "#placeholder" },
				{ "include": "#exposid" },
				{"include": "#var"}
			]
		},
		"grammar": {
			"begin": "\\[~(:opt)?(@(?:fake|loc|fmt|path|re))?\\b",
			"end": "\\]",
			"name": "markup.inline.tag.redoc",
			"contentName": "markup.grammarterm.redoc",
			"captures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "support.class.redoc" }
			},
			"patterns": [
			]
		},
		"var": {
			"begin": "\\[\\$",
			"end": "\\]",
			"name": "markup.inline.tag.redoc",
			"contentName": "markup.texvar.inline.redoc",
			"patterns": [
				{ "include": "#escape" },
				{"include": "#macro"},
				{ "include": "#placeholder" },
				{ "include": "#exposid" },
				{ "include": "#eval" },
				{"include": "#inline_code"}
			]
		},
		"placeholder": {
			"begin": "\\[\\^(:(?:oldconcept|newoldconcept|oc|newoc))?(@def)?\\b",
			"end": "\\]",
			"name": "markup.inline.tag.redoc",
			"contentName": "markup.placeholder.inline.redoc",
			"captures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "support.class.redoc" }
			},
			"patterns": [
			]
		},
		"exposid": {
			"begin": "\\[\\*((?::c)?)((?:@def)?)(?=\\b|\\W)",
			"end": "\\]",
			"name": "markup.inline.tag.redoc",
			"contentName": "markup.exposid.inline.redoc",
			"captures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "support.class.redoc" }
			},
			"patterns": [
			]
		},
		"macro": {
			"begin": "\\[=([a-zA-Z_][a-zA-Z0-9_]*|'|-{1,2}|`[|`])",
			"end": "\\]",
			"name": "markup.inline.tag.redoc",
			"contentName": "markup.macro.content.redoc",
			"captures": {
				"1": { "name": "markup.macro.name.redoc" }
			},
			"patterns": [
				{ "match": "[(,)]", "name": "markup.macro.name.redoc" },
				{ "include": "#escape" }
			]
		},
		"ref": {
			"match": "\\[#(?:(:note|:tab|:fig)#)?(?:[-a-zA-Z.:$0-9]+|(\\[=(\\w+)\\]))\\]",
			"name": "markup.ref.redoc",
			"captures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "markup.inline.tag.redoc" },
				"3": { "name": "markup.macro.name.redoc" }
			}
		},
		"cite": {
			"begin": "\\[cite\\b",
			"end": "\\]",
			"name": "markup.inline.tag.redoc",
			"contentName": "markup.cite.redoc",
			"patterns": [
				{"include": "#macro"}
			]
		},
		"url": {
			"begin": "\\[url\\b",
			"end": "\\]",
			"name": "markup.inline.tag.redoc",
			"contentName": "markup.url.redoc",
			"patterns": [
				{"include": "#macro"}
			]
		},
		"def": {
			"begin": "\\[\\+(:adj\\b|:%\\s)?",
			"end": "\\]",
			"name": "markup.inline.tag.redoc",
			"contentName": "markup.def.redoc",
			"captures": {
				"1": { "name": "support.function.redoc" }
			},
			"patterns": [
				{ "include": "#index" },
				{ "include": "#macro" },
				{ "include": "#inline_code" },
				{ "include": "#placeholder" },
				{ "include": "#grammar" }
			]
		},
		"quote": {
			"begin": "\\[\"",
			"end": "\\]",
			"name": "markup.inline.tag.redoc",
			"contentName": "markup.quote.redoc",
			"patterns": [
				{ "include": "#index" },
				{ "include": "#var" },
				{ "include": "#inline_code" },
				{ "include": "#placeholder" },
				{ "include": "#exposid" },
				{ "include": "#macro" },
				{ "include": "#grammar" },
				{ "include": "#macro" }
			]
		},
		"index": {
			"begin": "\\[%(:[a-zA-Z]+)?((?:@(?:\\w+)(?:=(?:[^@#\\s]+))?)*)(#[-a-zA-Z.]+)?",
			"end": "\\]",
			"name": "markup.index.redoc",
			"beginCaptures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "support.class.redoc" },
				"3": { "name": "markup.name.redoc" }
			},
			"patterns": [
				{ "include": "#escape" },
				{"include": "#index"},
				{ "include": "#exposid" },
				{ "include": "#placeholder" },
				{"include": "#langtag"},
				{"include": "#inline_code"},
				{ "include": "#macro" },
				{ "include": "#var" },
				{"include": "#grammar"},
				{"include": "#subindex"},
				{"include": "#index_as"}
			],
			"x":1
		},
		"subindex": {
			"begin": "\\[!",
			"end": "\\]",
			"name": "markup.index.sub.redoc",
			"captures": {
			},
			"patterns": [
				{ "include": "#escape" },
				{ "include": "#exposid" },
				{"include": "#index"},
				{ "include": "#placeholder" },
				{"include": "#langtag"},
				{"include": "#inline_code"},
				{ "include": "#macro" },
				{ "include": "#var" },
				{ "include": "#comment" },
				{"include": "#grammar"},
				{"include": "#subindex"},
				{"include": "#index_as"}
			]
		},
		"index_as": {
			"begin": "\\[@",
			"end": "\\]"
		},
		"decl_block": {
			"name": "markup.heading",
			"begin": "\\[(section|chapter|document)(:[a-zA-Z]+)?(#[^@#\\s]+)?",
			"captures": {
				"2": { "name": "support.function.redoc" },
				"3": { "name": "markup.name.redoc" }
			},
			"end": "\\]",
			"patterns": [
				{ "include": "#langtag" },
				{ "include": "#ref" },
				{ "include": "#exposid" },
				{ "include": "#inline_code" },
				{ "include": "#eval" },
				{ "include": "#placeholder" },
				{ "include": "#grammar" },
				{ "include": "#macro" },
				{ "include": "#comment" },
				{ "include": "#index" },
				{ "include": "#decl_attribute" }
			]
		},
		"decl_attribute": {
			"name" :"markup.heading.attribute",
			"begin": "\\[attribute#\\w+",
			"end": "\\]",
			"patterns": [
				{ "include": "#langtag" },
				{ "include": "#decl_value" }
			]
		},
		"decl_value": {
			"name" :"markup.heading.attribute.value",
			"begin": "\\[value",
			"end": "\\]",
			"contentName": "markup.heading.attribute.value",
			"patterns": [
				{ "include": "#langtag" },
				{ "include": "#inlinetag" }
			]
		},
		"directive": {
			"name": "markup.directive",
			"match": "\\[include(:[a-zA-Z]+)?((?:@(?:\\w+)(?:=(?:[^@#\\s]+))?)*)(\\s*\\b([a-zA-Z0-9.-]+))?\\]",
			"captures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "support.class.redoc" },
				"4": { "name": "support.class.redoc" }
			}
		},
		"region_block_note": {
			"name": "markup.region.tag.redoc",
			"contentName": "markup.region.note.redoc",
			"begin": "\\[begin(:note)(@defnote)?(\\b[\\w\\s]+)?\\s*\\]",
			"end": "\\[end(\\1)\\]",
			"captures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "support.class.redoc" },
				"3": { "name": "markup.region.note.redoc" }
			},
			"patterns": [
				{ "include": "$self" }
			]
		},
		"region_block_example": {
			"name": "markup.region.tag.redoc",
			"contentName": "markup.region.example.redoc",
			"begin": "\\[begin(:example)\\]",
			"end": "\\[end(\\1)\\]",
			"captures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "support.function.redoc" }
			},
			"patterns": [
				{ "include": "$self" }
			]
		},
		"region_block_desc": {
			"name": "markup.region.tag.redoc",
			"contentName": "markup.region.description.redoc",
			"begin": "\\[begin(:description)\\]",
			"end": "\\[end(\\1)\\]",
			"captures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "support.function.redoc" }
			},
			"patterns": [
				{ "include": "$self" }
			]
		},
		"region_footnote": {
			"name": "markup.region.tag.redoc",
			"contentName": "markup.region.footnote.redoc",
			"begin": "\\[note(:foot)(#[a-zA-Z.:$0-9]+)\\]",
			"end": "\\[note:end\\]",
			"captures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "markup.name.redoc" }
			},
			"patterns": [
				{ "include": "$self" }
			]
		},
		"codeblock": {
			"name": "markup.region.codeblock.redoc",
			"contentName": "meta.embedded.block.cpp",
			"begin": "\\[codeblock(?!:literal|:notation|:output)(:synopsis|:declaration)?.*\\]",
			"end": "\\[codeblock:end\\]",
			"captures": {
				"1": { "name": "support.function.redoc" }
			},
			"patterns": [
				{ "include": "source.cpp"}
			]
		},
		"codeblock_literal": {
			"name": "markup.region.codeblock.redoc",
			"contentName": "meta.embedded.pre",
			"begin": "\\[codeblock(:literal|:notation|:output)\\b.*\\]",
			"end": "\\[codeblock:end\\]",
			"captures": {
				"1": { "name": "support.function.redoc" }
			},
			"patterns": [
				{ "include": "#macro" },
				{ "include": "#span" },
				{ "include": "#exposid" },
				{ "include": "#escape" },
				{ "include": "#var" },
				{ "include": "#placeholder" }
			]
		},
		"syntax": {
			"name": "markup.region.tag.redoc",
			"contentName": "markup.region.syntax.redoc",
			"begin": "\\[syntax(:explanation)?.*\\]",
			"end": "\\[syntax:end\\]",
			"captures": {
				"1": { "name": "support.function.redoc" }
			},
			"patterns": [
				{ "include": "#inline_code" },
				{ "include": "#comment" },
				{ "include": "#macro" },
				{ "include": "#var" },
				{ "include": "#index" },
				{ "include": "#placeholder" },
				{ "include": "#exposid" },
				{ "include": "#grammarrule" },
				{ "include": "#grammarrule_alt" },
				{ "include": "#grammarrule_alt_desc" },
				{ "include": "#langtag" },
				{ "include": "#grammar" }
			]
		},
		"table": {
			"name": "markup.region.tag.redoc",
			"contentName": "markup.region.table.redoc",
			"begin": "\\[table(:listing|:grid)((?:@(?:\\w+)(?:=(?:[^@#\\s]+))?)*)(#[a-zA-Z.:$0-9]+)",
			"end": "\\[table:end\\]",
			"captures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "support.class.redoc" },
				"3": { "name": "markup.name.redoc" }
			},
			"patterns": [
				{ "include": "$self" },
				{ "match": "\\[-\\]", "name": "markup.table.cell.redoc"},
				{
					"begin": "\\[\\|((?:@\\w+(?:=[^@#\\s\\]]+)?)*)",
					"end": "\\]",
					"name": "markup.table.cell.redoc",
					"contentName": "markup.region.table.redoc",
					"captures": {
						"1": { "name": "support.class.redoc" }
					},
					"patterns": [
						{ "include": "#inline_code" },
						{ "include": "#inlinetag" },
						{ "include": "#placeholder" },
						{ "include": "#exposid" },
						{ "include": "#macro" },
						{ "include": "#langtag" },
						{ "include": "#grammar" }
					]
				}
			]
		},
		"math": {
			"name": "markup.region.tag.redoc",
			"contentName": "markup.region.math.redoc",
			"begin": "\\[math(:aligned)?\\]",
			"end": "\\[math:end\\]",
			"captures": {
				"1": { "name": "support.function.redoc" }
			},
			"patterns": [
				{ "include": "$self" }
			]
		},
		"grammarrule": {
			"name": "markup.region.syntax.redoc.grammarrul",
			"contentName": "markup.grammarterm.redoc",
			"begin": "\\[rule(?:end|(?:(:descriptive|:oneof)?(@\\w+)?)\\b)",
			"end": "\\]",
			"captures": {
				"1": { "name": "support.function.redoc" },
				"2": { "name": "support.class.redoc" }
			},
			"patterns": [
				{ "include": "#inline_code" },
				{ "include": "#macro" },
				{ "include": "#inlinetag" },
				{ "include": "#langtag" },
				{ "include": "#grammar" }
			]
		},
		"grammarrule_alt": {
			"begin": "\\[\\|\\s",
			"end": "\\]",
			"name": "markup.region.syntax.redoc.grammarrule.alt",
			"patterns": [
				{ "include": "#inline_code" },
				{ "include": "#macro" },
				{ "include": "#langtag" },
				{ "include": "#grammar" },
				{
					"match": "[a-zA-Z_-]+",
					"name": "markup.grammarterm.redoc"
				}
			]
		},
		"grammarrule_alt_desc": {
			"begin": "\\[\\|(:descriptive)",
			"end": "\\]",
			"name": "markup.region.syntax.redoc.grammarrule.desc",
			"captures": {
				"1": { "name": "support.function.redoc" }
			},
			"patterns": [
				{ "include": "#inline_code" },
				{ "include": "#inlinetag" },
				{ "include": "#macro" },
				{ "include": "#langtag" },
				{ "include": "#eval" },
				{ "include": "#grammar" }
			]
		},
		"inlinetag": {
			"name": "keyword.inlinetag",
			"match": "\\[(br)\\]"
		},
		"langtag": {
			"name": "keyword.langtag",
			"match": "\\[:(|zh_CN|en)(@~)?\\]"
		},
		"blocktag": {
			"name": "keyword.blocktag",
			"begin": "\\[(para|list|item|div)(\\:[~a-zA-Z]+)?((?:@(?:\\w+)(?:=(?:[^@#\\s]+))?)*)(#[a-zA-Z.]+)?",
			"end": "\\]",
			"captures": {
				"2": { "name": "support.function.redoc" },
				"3": { "name": "support.class.redoc" },
				"4": { "name": "markup.name.redoc" }
			},
			"patterns": [
				{ "include": "#langtag" },
				{ "include": "#ref" },
				{ "include": "#exposid" },
				{ "include": "#inline_code" },
				{ "include": "#eval" },
				{ "include": "#placeholder" },
				{ "include": "#grammar" },
				{ "include": "#macro" },
				{ "include": "#comment" },
				{ "include": "#index" }
			]
		},
		"span": {
			"name": "markup.span",
			"contentName": "meta.span.inner",
			"begin": "\\[span((?:@(?:\\w+)(?:=(?:[^@#\\s]+))?)*)(:\\w+)?\\b",
			"end": "\\]",
			"captures": {
				"1": { "name": "support.class.redoc" },
				"2": { "name": "support.function.redoc" }
			},
			"patterns": [
				{ "include": "#langtag" },
				{ "include": "#ref" },
				{ "include": "#var" },
				{ "include": "#exposid" },
				{ "include": "#inline_code" },
				{ "include": "#eval" },
				{ "include": "#placeholder" },
				{ "include": "#grammar" },
				{ "include": "#macro" },
				{ "include": "#comment" },
				{ "include": "#index" }
			]
		},
		"keywords": {
			"patterns": [{
				"name": "keyword.control.redoc",
				"match": "\\b(if|while|for|return)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.redoc",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.redoc",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "text.redoc"
}